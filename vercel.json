{
  "headers": [
    {
      "source": "/_assets/:file(.+\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [{ "source": "/:path(?!_assets)(.*)", "destination": "/index.html" }]
}
